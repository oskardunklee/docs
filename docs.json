{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Stanna Docs",
  "colors": {
    "primary": "#0EA5E9",
    "light": "#7DD3FC",
    "dark": "#0369A1"
  },
  "favicon": "/favicon.svg",
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      { "label": "Support", "href": "mailto:support@gostanna.com" }
    ],
    "primary": {
      "type": "button",
      "label": "Open app",
      "href": "https://app.gostanna.com"
    }
  },
  "contextual": { "options": ["copy", "view", "chatgpt", "claude", "perplexity", "mcp", "cursor", "vscode"] },
  "styling": { "eyebrows": "breadcrumbs" },
  "navigation": {
    "global": {
      "anchors": [
        { "anchor": "Website", "href": "https://gostanna.com", "icon": "globe" },
        { "anchor": "Open app", "href": "https://app.gostanna.com", "icon": "arrow-up-right-from-square" }
      ]
    },
    "tabs": [
      {
        "tab": "Getting Started",
        "groups": [
          {
            "group": "Quick Start",
            "pages": [
              "index",
              "getting-started/setup",
              "getting-started/first-login",
              "getting-started/dashboard-overview",
              "getting-started/api-keys"
            ]
          }
        ]
      },
      {
        "tab": "User Guides",
        "groups": [
          {
            "group": "For CS Professionals",
            "pages": [
              "user-guides/cs-pro/client-management"
            ]
          },
          {
            "group": "Core Concepts",
            "pages": [
              "pages/workspaces",
              "pages/health-score",
              "pages/sentiment"
            ]
          }
        ]
      },
      {
